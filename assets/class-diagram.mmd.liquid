classDiagram
direction TB

{%- assign mainNodeShapes = nodeShapes | where: "name" -%}
{% for nodeShape in mainNodeShapes %}
    class {{ nodeShape.name }} {
        {% if nodeShape.nodeKind == "IRI" -%}**URI**&nbsp;{%- endif -%}
    {%- for property in nodeShape.property -%}
        {%- assign name = property.path | replace: "schema:", "" %}

        {%- if property.or -%}
            {%- for or in property.or -%}
                {%- if or.node -%}
                    {{ or.node }}
                {%- elsif or.nodeKind -%}
                    {{ or.nodeKind }}
                {%- endif -%}
            {% endfor %}
        {%- endif %}
        **{{ name }}**: {% if property.datatype -%}
            {{ property.datatype }}
        {%- elsif property.nodeKind -%}
            {{- property.nodeKind -}}
        {%- elsif property.node -%}
            {{ property.node.class | default: property.node }}
        {%- endif %} [
        {%- if property.minCount == property.maxCount -%}
             {{- property.minCount -}}
        {%- else -%}
            {{ property.minCount }}..{{ property.maxCount | default: "*" }}
        {%- endif -%}
        ]
    {%- endfor %}
    }
{% endfor %}

